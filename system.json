{
  "id": "gfl5r",
  "title": "GFL5R",
  "description": "A shoddy attempt to port L5R's dice system and techniques to a near-future setting.",
  "version": "0.3.11",
  "compatibility": {
    "minimum": "12",
    "verified": "13"
  },
  "authors": [{ "name": "GFL5R Team" }],
  "esmodules": ["module/main.js"],
  "styles": ["bootstrap.min.css", "gfl5r.css"],
  "scripts": ["bootstrap.bundle.min.js"],
  "gridDistance": 1,
  "gridUnits": "meter",
  "template": "template.json",
  "documentTypes": {
    "Actor": ["character", "npc"],
    "Item": ["ability", "weaponry", "armor", "narrative", "item", "generic", "discipline", "module", "condition"]
  },
  "url": "https://github.com/GFL5R/GFL5R-Foundry",
  "manifest": "https://raw.githubusercontent.com/GFL5R/GFL5R-Foundry/refs/heads/main/system.json",
  "download": "https://github.com/GFL5R/GFL5R-Foundry/archive/refs/heads/main.zip",
  "license": "https://raw.githubusercontent.com/GFL5R/GFL5R-Foundry/refs/heads/main/LICENSE.md",
  "initiative": "1d6 + @skills.tactics",
  "grid": {
    "distance": 1,
    "units": "meter"
  }
}
